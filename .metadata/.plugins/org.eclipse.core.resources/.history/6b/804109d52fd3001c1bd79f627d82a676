import java.util.Scanner;

public class starPrint 
{
	public static void main(String[] args) 
	{
		System.out.print("Enter number of rows=");
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		for(int i=0;i<=n;i++)
			{
				
			for(int k=0;k<n;k++)
				{               
					System.out.print("*");
				}
			System.out.println();
			}
	}

}
