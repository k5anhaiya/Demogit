
public class app2
{
	public static void main(String[] args) 
	{
		int [] ar1= {6,2,4,12,8};
		int first,second,temp,size=ar1.length;
		for(int i=0;i<size;i++)
		{
		for(int j=1;j<size;j++)
			{
			first=ar1[i];
			second=ar1[j];
			if(first>second)
				{
					temp=first;
					first=second;
					second=temp;
				}
			}
		System.out.println(ar1[size-2]);
		}
	}

}
