
public class app2
{
	public static void main(String[] args) 
	{
		int [] ar1= {2,4,6,8,12};
		int [] ar2= {4,9,13,12,15};
		int [] ar3= {};
		int first,second,third;
		for(int i=0;i<ar1.length;i++)
		{
		first=ar1[i];
		second=ar2[i];
		if(ar1[i]==ar2[i])
		{
			ar3[i]=ar1[i];
		}
		else
		{
			ar3[i]=ar1[i];
			ar3[i+1]=ar2[i+1];
		}
		}
	}

}
