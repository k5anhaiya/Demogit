public class app2
{
	public static void main(String[] args) 
	{
		int [] arr= {50,20,30,8,12,56,45,56};
		int size=arr.length;
		int first,second,temp;
			
		
		for(int i=0;i<arr.length;i++)
		{
			first=arr[i];
			for(int j=1;j<arr.length;i++)
			{	
				second=arr[j];
				if(first>second)
				{
					temp=first;
					first=second;
				}
			}
		}
		
	}
}